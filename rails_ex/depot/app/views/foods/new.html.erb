<h1>商品新規作成</h1>

<% form_for(@food) do |f| %>
  <%= f.error_messages %>

  <p>
    <b>商品名:</b><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <b>商品紹介:</b><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <b>商品図像:</b><br />
    <%= f.text_field :image_url %>
  </p>
   <p>
    <b>商品価額:</b><br />
    <%= f.text_field :price %>
  </p>
  <p>
    <%= f.submit "新規" %>
  </p>
<% end %>

<%= link_to '戻る', foods_path %>
